{
  "name": "@bluewavelabs/prism-ui",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "author": "BlueWave Labs",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.cts"
    },
    "./style": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./global.css": {
      "default": "./dist/style/globals.css"
    },
    "./_variables.css": {
      "default": "./dist/style/_variables.css"
    },
    "./theme.css": {
      "default": "./dist/style/theme.css"
    }
  },
  "sideEffects": [
    "dist/**/*",
    "./src/**/*.css"
  ],
  "keywords": [
    "design-system",
    "ui",
    "components"
  ],
  "homepage": "./",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:tokens": "npx style-dictionary build --config ./sd.config.json",
    "prepare": "husky",
    "build:pkg": "tsup",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./docs"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@radix-ui/react-tabs": "^1.1.4",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.488.0",
    "next": "^15.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanitize-html": "^2.15.0",
    "shiki": "^3.2.2",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/experimental-nextjs-vite": "^8.6.12",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/sanitize-html": "^2.15.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "playwright": "^1.51.1",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "storybook": "^8.6.12",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "exclude": [
      "node_modules",
      "build",
      "dist",
      ".next",
      "src/sd/**",
      "src/components/ui/**"
    ]
  },
  "style": "src/styles/globals.css",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
